    "blockchain",
    "love"
  ],
  "main": "src/index.tsx",
  "dependencies": {
    "@chakra-ui/react": "^2.4.9",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@solana/web3.js": "1.73.0",
    "chakra-react-select": "^4.4.3",
    "ethers": "^5.7.2",
    "framer-motion": "^8.5.3",
